{
	"my example flow": {
		"description": "a simple flow for testing",
		"endpoints": {
			"inFile": {
				"passive": true,
				"active": false,
				"in": true,
				"out": false
			}
		}
		"steps": {
			"my_file_reader": {
				"type": "kronos-adapter-inbound-file",
				"config": {
					"watchDir": "/tmp/dropZone"
				},
				"endpoints": {
					"out": "step:my_unzip/inUnZip"
				}
			},
			"my_unzip": {
				"type": "kronos-step-gzip"
			},
			"my_untar": {
				"type": "kronos-step-archive-untar"
			},
			"endpoints": {
				"inUntar": "step:my_unzip/outUnZip"
			}
		},
		"inbound_steps": [
			"my_file_reader"
		]
	}
}
