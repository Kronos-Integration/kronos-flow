{
  "name": "kronos-flow",
  "version": "0.0.0-semantic-release",
  "description": "Base flow implementation",
  "keywords": ["kronos-step"],
  "main": "dist/flow.js",
  "module": "src/flow.js",
  "scripts": {
    "cover": "nyc ava",
    "test": "ava",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "precover": "rollup -c tests/rollup.config.js",
    "pretest": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "docs": "jsdoc2md --configure doc/jsdoc.json -l off -t doc/README.hbs -f src/*.js >README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kronos-Integration/kronos-flow.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-flow/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-flow#readme",
  "engines": {
    "node": ">=8.4.0"
  },
  "devDependencies": {
    "kronos-message": "^2.1.3",
    "kronos-service-manager": "^3.5.10",
    "kronos-step-passthrough": "^2.0.15",
    "kronos-test-step": "^3.1.4",
    "semantic-release": "^8.0.0",
    "jsdoc-to-markdown": "^3.0.0",
    "jsdoc-babel": "^0.3.0",
    "markdown-doctest": "^0.9.1",
    "nyc": "^11.2.1",
    "ava": "^0.22.0",
    "rollup": "^0.49.3",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-multi-entry": "^2.0.1",
    "babel-preset-stage-3": "^6.24.1",
    "xo": "^0.19.0"
  },
  "dependencies": {
    "kronos-endpoint": "^2.14.10",
    "kronos-step": "^5.3.8"
  },
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "xo": {
    "space": true
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  },
  "ava": {
    "files": ["build/test-bundle.js"],
    "presets": ["stage-3"],
    "require": ["babel-register"]
  },
  "nyc": {
    "include": ["build/test-bundle.js"],
    "reporter": ["lcov"],
    "report-dir": "./build/coverage"
  }
}
