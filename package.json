{
  "name": "kronos-flow",
  "version": "0.0.0-semantic-release",
  "description": "Base flow implementation",
  "keywords": [
    "kronos-step"
  ],
  "main": "dist/flow.js",
  "module": "src/flow.js",
  "scripts": {
    "cover": "nyc --temp-directory build/nyc ava",
    "test": "ava",
    "semantic-release": "semantic-release",
    "precover": "rollup -c tests/rollup.config.js",
    "pretest": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "docs": "jsdoc2md --configure doc/jsdoc.json -l off -t doc/README.hbs -f src/*.js >README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kronos-Integration/kronos-flow.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-flow/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-flow#readme",
  "engines": {
    "node": ">=8.9.3"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "jsdoc-babel": "^0.3.0",
    "jsdoc-to-markdown": "^3.0.2",
    "kronos-service-manager": "^3.5.10",
    "kronos-step-passthrough": "^2.0.15",
    "kronos-test-step": "^3.1.4",
    "markdown-doctest": "^0.9.1",
    "nyc": "^11.4.0",
    "rollup": "^0.52.1",
    "semantic-release": "^10.0.1",
    "xo": "^0.19.0",
    "babel-preset-env": "^1.6.1"
  },
  "dependencies": {
    "kronos-endpoint": "^2.14.11",
    "kronos-step": "^5.4.0"
  },
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "xo": {
    "space": true
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  },
  "ava": {
    "files": [
      "build/*-test.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/*-test.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  }
}
