{
  "name": "kronos-flow",
  "version": "0.0.0",
  "description": "Base flow implementation",
  "keywords": [
    "kronos-step"
  ],
  "main": "index.js",
  "scripts": {
    "cover": "node ./node_modules/istanbul/lib/cli.js cover --hook-run-in-context ./node_modules/mocha/bin/_mocha -- --R spec --U exports tests",
    "doc": "jsdoc lib/*.js",
    "test": "mocha tests/*_test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kronos-Integration/kronos-flow.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-flow/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-flow#readme",
  "engines": {
    "node": ">=4.2.1"
  },
  "devDependencies": {
    "chai": "3.4.1",
    "istanbul": "^0.4.2",
    "cz-conventional-changelog": "^1.1.5",
    "kronos-message": "^1.0.1",
    "kronos-step-passthrough": "^1.2.2",
    "kronos-test-step": "2.9.1",
    "mocha": "2.3.4",
    "semantic-release": "6.0.3"
  },
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "dependencies": {
    "kronos-step": "^3.5.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
